python@ubuntu:~/week3$ cat 3-2-4.c 3-2-5.c
#include "ch02.h"
int main(int argc, char *argv[])
{
    FILE *fp;
    if((fp = fopen("./test1.txt","w"))==NULL)
        perror("open failed!\n");
    int i;
    for(i=1;i<argc;i++)
    {
        fprintf(fp,"%s",argv[i]);
        printf("[%d] : \t %s  \t%ldbyte\n",i,argv[i],strlen(argv[i]));
    }
    fclose(fp);
    return 0;
}
#include "ch02.h"
int main(int argc, char *argv[])
{
    FILE *fp;
    if((fp = fopen("./test1.txt","w"))==NULL)
        perror("open failed!\n");
    int i;
    for(i=1;i<argc;i++)
    {
        fwrite(argv[i],1,strlen(argv[i]),fp);
        printf("[%d] : \t %s  \t%ldbyte\n",i,argv[i],strlen(argv[i]));
    }
    fclose(fp);
    return 0;
}
