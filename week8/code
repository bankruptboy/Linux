#include <stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <unistd.h>
#include <string.h>
#include <fcntl.h>
#include <stdlib.h>
#include <time.h>
#include <dlfcn.h>
#include <setjmp.h>
#include <wait.h>
#include <errno.h>
void err_exit(char *s)
{
    printf("Usage : *s is faile!\n");
    exit(1);
}
#include "./ch09.h"
int main()
{
    pid_t pid;
    pid = fork();
    if(pid<0)
    {
        fprintf(stderr,"failed to fork.Reason(%s)\n",strerror(errno));
        exit(-1);
    }
    else if(pid == 0)
    {
        printf("child : pid = %d,ppid = %d return form fork =%d\n\n",getpid(),getppid(),pid);
        exit(0);
    }
    else
    {
        printf("parent : pid = %d,ppid = %d return from fork()=%d\n\n",getpid(),getppid(),pid);
        return 0;
    }
}
