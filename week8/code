python@ubuntu:~/week9$ cat 9-5.c
#include "./ch09.h"
int main()
{
    pid_t i;
    i = vfork();
    if(i<0)
    {
        perror("Vfork failed!\n");
        exit(-1);
    }
    else if(i == 0)
    {
        printf("child : pid = %d ppid = %d, i = %d\n",getpid(),getppid(),i);
        _exit(0);
       // return 0;
    }
    else
    {
        wait(NULL);
        printf("parent : pid = %d, ppid = %d , i =%d\n",getpid(),getppid(),i);
        return 0;
    }
}

