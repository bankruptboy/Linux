python@ubuntu:~/week9$ cat 9-2.c
#include "./ch09.h"
int g = 1;
int main()
{
    pid_t pid;
    int l = 99;
    int *pt=malloc(sizeof(int));
    char *string = "test string!";
    *pt = 9999;
    printf("----------Before Fork----------");
    pid = fork();
    if(pid<0)
    {
        fprintf(stderr,"failed to fork.Reason(%s)\n",strerror(errno));
        exit(-1);
    }
    else if(pid == 0)
    {
        printf("child : pid = %d,ppid = %d return form fork =%d\n",getpid(),getppid(),pid);
        printf("child : init g = %d, l = %d , *pt = %d\n",g,l,*pt);
        g++;
        l++;
        (*pt)++;
        printf("child : g = %d %x, l = %d %x, *pt = %d %x\n",g,&g,l,&l,*pt,pt);
        printf("chlid : string = %s\n",string);
        exit(0);
    }
    else
    {
        sleep(2);
        printf("parent : pid = %d,ppid = %d return from fork()=%d\n",getpid(),getppid(),pid);
        printf("child : init g = %d, l = %d , *pt = %d\n",g,l,*pt);
        g++;
        l++;
        (*pt)++;
        printf("child : g = %d %x, l = %d %x, *pt = %d %x\n",g,&g,l,&l,*pt,pt);
        printf("chlid : string = %s\n",string);
        return 0;
    }
}
